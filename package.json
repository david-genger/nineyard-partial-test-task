  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1101.0",
    "@angular/cli": "^11.1.0",
    "@angular/compiler-cli": "^11.1.0",
    "@angular/language-service": "^11.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.21",
    "codelyzer": "^6.0.0",
