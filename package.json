    "@ag-grid-enterprise/core": "^25.0.1",
    "@ag-grid-enterprise/filter-tool-panel": "^25.0.1",
    "@ag-grid-enterprise/menu": "^25.0.1",
    "@ag-grid-enterprise/multi-filter": "^25.0.1",
    "@ag-grid-enterprise/range-selection": "^25.0.1",
    "@ag-grid-enterprise/rich-select": "^25.0.1",
    "@ag-grid-enterprise/server-side-row-model": "^25.0.1",
    "@ag-grid-enterprise/set-filter": "^25.0.1",
    "@ag-grid-enterprise/side-bar": "^25.0.1",
    "@angular/animations": "^11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "^11.1.0",
    "@angular/compiler": "^11.1.0",
    "@angular/core": "^11.1.0",
    "@angular/forms": "^11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "^11.1.0",
    "@angular/platform-browser-dynamic": "^11.1.0",
    "@angular/router": "^11.1.0",
    "@microsoft/signalr": "^5.0.2",
    "@types/overlayscrollbars": "^1.9.0",
    "angular-cc-library": "^2.0.1",
    "angular-material-expansion-panel": "^0.7.2",
    "angular-resizable-element": "^3.3.2",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "crypto-js": "^3.3.0",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.20",
    "ng2-pdf-viewer": "^6.3.2",
