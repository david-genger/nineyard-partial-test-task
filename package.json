{
  "name": "datavanced",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.production.json",
    "dev": "ng serve --proxy-config src/proxy.conf.json",
    "hmr": "ng serve --proxy-config src/proxy.production.json --hmr",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod-build": "ng build --prod --aot && npm run rename-prod",
    "branch-build": "ng build --prod --aot && npm run rename-branch",
    "rename-prod": "rname -f dist/datavanced/_redirects-production _redirects",
    "rename-branch": "rname -f dist/datavanced/_redirects-dev _redirects"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/angular": "^25.0.1",
    "@ag-grid-community/core": "^25.0.1",
    "@ag-grid-enterprise/clipboard": "^25.0.1",
